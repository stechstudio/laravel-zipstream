{
    "name": "stechstudio/laravel-zipstream",
    "description": "Build",
    "keywords": [
        "stechstudio",
        "laravel-zipstream"
    ],
    "homepage": "https://github.com/stechstudio/laravel-zipstream",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Joseph Szobody",
            "email": "joseph@stechstudio.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "guzzlehttp/guzzle": "^6.0|^7.0",
        "illuminate/support": "^5.6|^6.0|^7.0|^8.0",
        "maennchen/zipstream-php": "^v2.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "STS\\ZipStream\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "STS\\ZipStream\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "STS\\ZipStream\\ZipStreamServiceProvider"
            ],
            "aliases": {
                "Zip": "STS\\ZipStream\\ZipStreamFacade"
            }
        }
    }
}
